Algoritmo "SomaDoisNumeros"

Var
    numero1, numero2, soma : inteiro

Inicio
       Escreval("Digite o Primeiro Número : ")
       Leia(numero1)
       Escreval("Digite o Segundo Número : ")
       Leia(numero2)
       
       soma <-numero1+numero2
       
       Escreval("A Soma Entre ", numero1, " e", numero2, " é igual a ", soma )

Fimalgoritmo